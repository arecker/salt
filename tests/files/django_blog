#!/usr/bin/env python
import requests


def main():
    response = requests.get('http://backend.alexrecker.com/api/')

    assert response.status_code is 200, 'response code is actually {}'.format(response.status_code)

    if not response.status_code is 200:
        print(response.content)

    data = response.json()

    for key in ['fortune-cookies', 'subscribers']:
        assert data.has_key(key)

    exit()


if __name__ == '__main__':
    main()
