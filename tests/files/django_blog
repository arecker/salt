#!/usr/bin/env python
import urllib2


def main():
    response = urllib2.urlopen('http://backend.alexrecker.com/api/')

    assert response.getcode() is 200, 'response code is actually {}'.format(response.getcode())

    data = response.read()

    for key in ['fortune-cookies', 'subscribers']:
        assert key in data

    exit()


if __name__ == '__main__':
    main()
